{
  "name": "bar-do-carneiro",
  "version": "1.0.0",
  "description": "Sistema de Gestão de Estoque e Financeiro - BAR DO CARNEIRO",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "build:prod": "node scripts/build-production.js"
  },
  "keywords": [
    "estoque",
    "financeiro",
    "gestão",
    "bar",
    "restaurante"
  ],
  "author": "ErycK",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.1.6"
  }
}

